dots_output_success() {
    printf "[ \033[00;32mOK\033[0m ] $1\n"
}

dots_output_finished() {
    printf "[ \033[00;32mFINISHED\033[0m ] $1\n"
}

dots_output_info() {
    printf "[\033[1;36m .. \033[0m] $1\n"
}

dots_output_warn() {
    printf "[\033[0;33mWARN\033[0m] $1\n"
}

dots_output_fail() {
    printf "[\033[0;31mFAIL\033[0m] $1\n"
}

